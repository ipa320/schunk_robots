<?xml version="1.0"?>
<launch>
    	<!-- define arguments -->
    	<arg name="robot" default="$(env ROBOT)"/>

	<group>
        	<include file="$(find schunk_arm_navigation)/launch/environment_server.xml">
			<arg name="robot" value="$(arg robot)"/>
		</include>
        	<include file="$(find schunk_arm_navigatio)/launch/ompl_planning.xml">
                	<arg name="robot" value="$(arg robot)"/>
        	</include>
        	<include file="$(find schunk_arm_navigatio)/launch/move_groups.xml">
                	<arg name="robot" value="$(arg robot)"/>
        	</include>
    	</group>
</launch>
