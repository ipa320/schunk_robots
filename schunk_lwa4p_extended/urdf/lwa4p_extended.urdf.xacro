<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find schunk_lwa4p_extended)/urdf/lwa4p_extended.gazebo.xacro" />
  <xacro:include filename="$(find schunk_lwa4p_extended)/urdf/lwa4p_extended.transmission.xacro" />

    
  <xacro:macro name="schunk_lwa4p_extended" params="parent name *origin arm_1_ref arm_2_ref arm_3_ref arm_4_ref arm_5_ref arm_6_ref arm_7_ref">


    <!-- joint between base_link and lwa4p_extended_connector_link -->
    <joint name="${name}_base_joint" type="fixed">
      <insert_block name="origin" />
      <parent link="${parent}"/>
      <child link="${name}_base_link"/>
    </joint>
    
    <link name="${name}_base_link">
      <xacro:cylinder_inertial radius="0.056" length="0.134" mass="2.0">
        <origin xyz="0 0 ${0.134/2}" rpy="0 0 0" />
      </xacro:cylinder_inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_base_link.stl" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_base_link.stl" />
        </geometry>
      </collision>
    </link>
    
    
    <!-- joint between arm_base_link and arm_1_link -->
    <joint name="${name}_1_joint" type="revolute" >
      <origin xyz="0 0 ${0.134+0.005}" rpy="0 0 0" />
      <parent link="${name}_base_link" />
      <child link="${name}_1_link" />
      <axis xyz="0 0 1"/>
      <calibration rising="${arm_1_ref}"/>
      <dynamics damping="10" />
      <limit effort="370" velocity="2.0" lower="-6.2831853" upper="6.2831853"/>
      <safety_controller k_position="20" k_velocity="50" soft_lower_limit="${-6.2831853 + 0.01}" soft_upper_limit="${6.2831853 - 0.01}" />
    </joint>

    <link name="${name}_1_link">
      <xacro:box_inertial x="0.01" y="0.01" z="0.01" mass="0.1">
        <origin xyz="0 0 0" rpy="0 0 0" />
      </xacro:box_inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <!--mesh filename="package://schunk_lwa4p_extended/meshes/arm_1_link.stl" /-->
          <box size="0.01 0.01 0.01"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <!--mesh filename="package://schunk_lwa4p_extended/meshes/arm_1_link.stl" /-->
          <box size="0.01 0.01 0.01"/>
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_1_link and arm_2_link -->  
    <joint name="${name}_2_joint" type="revolute">
      <origin xyz="0 0 0.0915" rpy="1.5708 0 0"/> 
      <parent link="${name}_1_link"/>
      <child link="${name}_2_link"/>
      <axis xyz="0 0 1"/>
      <calibration rising="${arm_2_ref}"/>
      <dynamics damping="10" />
      <limit effort="370" velocity="2.0" lower="-2.0943951" upper="2.0943951"/>
      <safety_controller k_position="20" k_velocity="50" soft_lower_limit="${-2.0943 + 0.01}" soft_upper_limit="${2.0943 - 0.01}" />
    </joint>

    <link name="${name}_2_link">
      <inertial>
        <origin xyz="0 0.018 0.018" rpy="0 0 0"/>
        <mass value="3.9"/>
        <inertia ixx="0.009780" ixy="0.0" ixz="0.0" iyy="0.013300" iyz="0.0" izz="0.009780" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_2_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_2_link.dae" />
        </geometry>
      </collision>
    </link>


    <!-- joint between arm_2_link and arm_3_link -->  
    <joint name="${name}_3_joint" type="revolute">
      <origin xyz="0 0 0" rpy="-1.5708 0 0"/>
      <parent link="${name}_2_link"/>
      <child link="${name}_3_link"/>
      <axis xyz="0 0 1"/>
      <calibration rising="${arm_3_ref}"/>
      <dynamics damping="5" />
      <limit effort="176" velocity="2.0" lower="-6.2831853" upper="6.2831853"/>
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-6.2831853 + 0.01}" soft_upper_limit="${6.2831853 - 0.01}" />
    </joint>

    <link name="${name}_3_link">
      <inertial>
        <origin xyz="0.0 0.06247 0.23425" rpy="0 0 0"/>
        <mass value="1.94533"/>
        <inertia ixx="0.0344" ixy="0.0" ixz="0.0077" iyy="0.0362" iyz="0.0" izz="0.0052" />
      </inertial>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.1"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_3_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_3_link.dae" />
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_3_link and arm_4_link -->
    <joint name="${name}_4_joint" type="revolute">
      <origin xyz="0 0 0.45360" rpy="1.5708 0 0" />
      <parent link="${name}_3_link"/>
      <child link="${name}_4_link"/>
      <axis xyz="0 0 1" />
      <calibration rising="${arm_4_ref}"/>
      <dynamics damping="5" />
      <limit effort="176" velocity="2.0" lower="-2.0943951" upper="2.0943951"/>
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-2.0943951 + 0.01}" soft_upper_limit="${2.0943951 - 0.01}" />
    </joint>

    <link name="${name}_4_link">
      <inertial>
        <origin xyz="0 0.018 -0.018" rpy="0 0 0"/>
        <mass value="3.9"/>
        <inertia ixx="0.009780" ixy="0.0" ixz="0.0" iyy="0.013300" iyz="0.0" izz="0.009780" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_4_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_4_link.dae" />
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_4_link and arm_5_link -->
    <joint name="${name}_5_joint" type="revolute">
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
      <parent link="${name}_4_link"/>
      <child link="${name}_5_link"/>
      <axis xyz="0 0 1" />
      <calibration rising="${arm_5_ref}"/>
      <dynamics damping="5" />
      <limit effort="41.6" velocity="2.0" lower="-6.2831853" upper="6.2831853" />
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-6.2831853 + 0.01}" soft_upper_limit="${6.2831853 - 0.01}" />
    </joint>

    <link name="${name}_5_link">
      <inertial>
        <origin xyz="0.0 0.04175 0.17026" rpy="0 0 0"/>
        <mass value="1.3297"/>
        <inertia ixx="0.01" ixy="0.0" ixz="-0.0031" iyy="0.0111" iyz="0.0" izz="0.0031" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_5_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_5_link.dae" />
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_5_link and arm_6_link -->
    <joint name="${name}_6_joint" type="revolute">
      <origin xyz="0 0 0.2986" rpy="1.5708 0 0" />
      <parent link="${name}_5_link"/>
      <child link="${name}_6_link"/>
      <axis xyz="0 0 1" />
      <calibration rising="${arm_6_ref}"/>
      <dynamics damping="5" />
      <limit effort="20.1" velocity="2.0" lower="-2.0943951" upper="2.0943951" />
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-2.0943951 + 0.01}" soft_upper_limit="${2.0943951 - 0.01}" />
    </joint>

    <link name="${name}_6_link">
      <inertial>
        <origin xyz="0 0.012 -0.012" rpy="0 0 0"/>
        <mass value="1.9"/>
        <inertia ixx="0.002750" ixy="0.0" ixz="0.0" iyy="0.004002" iyz="0.0" izz="0.002830" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_6_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://schunk_lwa4p_extended/meshes/arm_6_link.dae" />
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_6_link and arm_7_link -->
    <joint name="${name}_7_joint" type="revolute">
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
      <parent link="${name}_6_link"/>
      <child link="${name}_7_link"/>
      <axis xyz="0 0 1" />
      <calibration rising="${arm_7_ref}"/>
      <dynamics damping="5" />
      <limit effort="20.1" velocity="2.0" lower="-2.0943951" upper="2.0943951" />
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-2.0943951 + 0.01}" soft_upper_limit="${2.0943951 - 0.01}" />
    </joint>

    <link name="${name}_7_link">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.1"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
      </inertial>
    </link>
    
    <!--link name="${name}_7_link">
      <xacro:box_inertial x="0.01" y="0.01" z="0.01" mass="0.1">
        <origin xyz="0 0 0" rpy="0 0 0" />
      </xacro:box_inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <box size="0.01 0.01 0.01"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <box size="0.01 0.01 0.01"/>
        </geometry>
      </collision>
    </link-->



    <!-- extensions -->
    <xacro:schunk_lwa4p_extended_gazebo name="${name}" />
    <xacro:schunk_lwa4p_extended_transmission name="${name}" />

  </xacro:macro>

</robot>
